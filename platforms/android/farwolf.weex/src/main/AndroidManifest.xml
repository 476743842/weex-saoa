<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.farwolf.weex">
    <application>
        <activity android:name="com.farwolf.weex.activity.QrActivity_"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity android:name="com.farwolf.weex.activity.PresentActivity_"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"
            >
        </activity>
        <activity
            android:name="com.farwolf.qrcode.zxing.android.CaptureActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity android:name="com.farwolf.weex.activity.EntryActivity_"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity android:name="com.farwolf.weex.activity.WeexActivity_"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity android:name="com.farwolf.weex.activity.LanscapeActivity_"
            android:windowSoftInputMode="adjustPan"
            android:theme="@style/AppTheme"
            android:screenOrientation="landscape"
            >
        </activity>
        <activity android:name="com.farwolf.weex.activity.SplashActivity_"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing"
            >
        </activity>

    </application>

</manifest>
